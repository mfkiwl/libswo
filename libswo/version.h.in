#ifndef LIBSWO_VERSION_H
#define LIBSWO_VERSION_H

/**
 * @file
 *
 * Package and library version macros.
 */

/** Major version number of the libswo package. */
#define LIBSWO_VERSION_PACKAGE_MAJOR @LIBSWO_VERSION_PACKAGE_MAJOR@

/** Minor version number of the libswo package. */
#define LIBSWO_VERSION_PACKAGE_MINOR @LIBSWO_VERSION_PACKAGE_MINOR@

/** Micro version number of the libswo package. */
#define LIBSWO_VERSION_PACKAGE_MICRO @LIBSWO_VERSION_PACKAGE_MICRO@

/** Version number string of the libswo package. */
#define LIBSWO_VERSION_PACKAGE_STRING "@LIBSWO_VERSION_PACKAGE@"

/** <i>Current</i> version number of the libswo libtool interface. */
#define LIBSWO_VERSION_LIB_CURRENT @LIBSWO_VERSION_LIB_CURRENT@

/** <i>Revision</i> version number of the libswo libtool interface. */
#define LIBSWO_VERSION_LIB_REVISION @LIBSWO_VERSION_LIB_REVISION@

/** <i>Age</i> version number of the libswo libtool interface. */
#define LIBSWO_VERSION_LIB_AGE @LIBSWO_VERSION_LIB_AGE@

/** Version number string of the libswo libtool interface. */
#define LIBSWO_VERSION_LIB_STRING "@LIBSWO_VERSION_LIB@"

#endif /* LIBSWO_VERSION_H */
